    "from phi.agent import Agent, RunResponse\n",
    "from phi.model.groq import Groq\n",
    "\n",
    "agent = Agent(\n",
    "    model=Groq(id=\"llama-3.3-70b-versatile\"),\n",
    "    markdown=True\n",
    ")\n",
    "\n",
    "# Get the response in a variable\n",
    "run: RunResponse = agent.run(\"Share a 2 sentence horror story.\")\n",
    "print(run.content)"
   ]
  }
